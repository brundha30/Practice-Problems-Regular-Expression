   "source": [
    "# Write a Python program that matches a word at the end of string, with optional punctuation.\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the word:funny girl\n",
      "match found\n"
     ]
    }
   ],
   "source": [
    "import re\n",
    "\n",
    "word = input(\"Enter the word:\")\n",
    "\n",
    "match = re.search(\"girl$\",word)\n",
    "\n",
    "if match!=None:\n",
    "    print(\"match found\")\n",
    "else:\n",
    "    print(\"match not found\")\n",
    "\n"
   ]
  }
